/* dimensions of elements in Atomic */
size = 140px
electron = round(size / (1 + 5 ** .5))
orbital = round(size / 17)
font = round(size / 10)
total = size + 2 * orbital
n = 9
external = true

/* colors #F39C12*/
text = #333
border = #ECF0F1
inner = text
bg = rgba(241, 196, 15, .9)
highlight = bg

/* dimension of the articles */
height = 50%
